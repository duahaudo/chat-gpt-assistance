{
  "name": "chatgpt-assistant",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "cd client && npm run build",
    "dev": "concurrently \"cd client && npm start\" \"cd server && npm run start\"",
    "start": "cd server && npm start",
    "bootstrap": "npm install && (cd client && npm install) && (cd server && npm install)"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "author": {
    "name": "Stiger",
    "email": "StigerDieu@gmail.com"
  }
}